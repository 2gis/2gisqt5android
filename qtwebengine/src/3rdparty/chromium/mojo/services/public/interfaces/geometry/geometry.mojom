module mojo {

struct Point {
  int32 x;
  int32 y;
};

struct PointF {
  float x;
  float y;
};

struct Size {
  int32 width;
  int32 height;
};

struct Rect {
  int32 x;
  int32 y;
  int32 width;
  int32 height;
};

struct Transform {
  // Should have exactly 16 entries.
  float[] matrix;
};

}

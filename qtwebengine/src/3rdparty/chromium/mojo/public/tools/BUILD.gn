import("//mojo/public/mojo.gni")

if (use_prebuilt_mojo_shell) {
  copy("copy_mojo_shell") {
    filename = "mojo_shell"
    if (is_win) {
      filename += ".exe"
    }
    sources = [ "prebuilt/$filename" ]
    outputs = [ "$root_out_dir/$filename" ]
  }
}
